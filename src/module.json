{
  "id": "revolutionary-piratenames",
  "title": "Pirate Name Generator",
  "description": "This module provides roll tables, API methods, and a macro to generate personal and ship names for the major European powers in the Caribbean during the Golden Age of Piracy: the Spanish Empire, the British Empire, the Kingdom of France, and the Dutch West India Company.",
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": "13",
    "verified": "13"
  },
  "authors": [
    {
      "name": "Jason Godesky",
      "email": "jason@revolutionarygames.co",
      "github": "revolutionarygamesco"
    }
  ],
  "esmodules": [
    "scripts/module.js"
  ],
  "packs": [
    {
      "name": "piratenames-tables",
      "label": "Pirate Name Roll Tables",
      "path": "packs/rolltables",
      "type": "RollTable",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "piratenames-macros",
      "label": "Pirate Name Macros",
      "path": "packs/macros",
      "type": "Macro",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    }
  ],
  "packFolders": [
    {
      "name": "Pirate Name Generator",
      "sorting": "a",
      "packs": [
        "piratenames-tables",
        "piratenames-macros"
      ]
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "includes": [],
  "socket": false,
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "bugs": "#{BUGS}#",
  "changelog": "#{CHANGELOG}#",
  "license": "#{LICENSE}#",
  "readme": "#{README}#",
  "funding": [
    {
      "type": "patreon",
      "url": "https://patreon.com/revolutionarygamesco"
    }
  ]
}
